{
    "name": "opera-project/media-bundle",
    "description": "Easy manage media with opera cms",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric Lombardot",
            "email": "cedric.lombardot@gmail.com"
        }
    ],
    "require": {
        "opera-project/core-bundle": "^0.1",
        "knplabs/knp-gaufrette-bundle": "~0.5",
        "liip/imagine-bundle": "^2.1",
        "white-october/pagerfanta-bundle": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Opera\\MediaBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
